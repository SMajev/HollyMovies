{
    "name": "d-fi",
    "version": "2.1.3",
    "description": "A streaming music downloader.",
    "author": "Sayem Chowdhury",
    "license": "MIT",
    "scripts": {
        "lint": "eslint .",
        "prepublishOnly": "eslint . && tsc",
        "prebuild": "eslint .",
        "build": "tsc",
        "postinstall": "node setup-termux.js",
        "dev": "ts-node src/d-fi.ts",
        "test": "SIMULATE=true ava"
    },
    "bin": "dist/src/d-fi.js",
    "repository": {
        "type": "git",
        "url": "https://notabug.org/sayem314/d-fi.git"
    },
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "adm-zip": "^0.5.9",
        "chalk": "^4.1.2",
        "commander": "^8.2.0",
        "d-fi-core": "^1.3.2",
        "dot-prop": "^6.0.1",
        "got": "^11.8.1",
        "gradient-string": "^1.2.0",
        "log-update": "^4.0.0",
        "p-queue": "^6.6.2",
        "prompts": "^2.4.2",
        "true-case-path": "^2.2.1",
        "update-check": "^1.5.4"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.4.34",
        "@types/gradient-string": "^1.1.2",
        "@types/node": "^16.11.0",
        "@types/prompts": "^2.0.14",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "ava": "^3.15.0",
        "eslint": "^7.32.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.1",
        "ts-node": "^10.3.0",
        "typescript": "^4.4.4"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "files": [
            "!dist"
        ],
        "require": [
            "ts-node/register"
        ],
        "timeout": "5m",
        "serial": true,
        "verbose": true
    }
}
